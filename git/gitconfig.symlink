	name = Vincent Pretre
	email = vincent.pretre@gmail.com
[core]
	editor = emacs
[color]
	ui = true
[alias]
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%aN, %cr)%Creset' --abbrev-commit --date=relative
	amend = commit --amend
	st = status
[pull]
	rebase = false
